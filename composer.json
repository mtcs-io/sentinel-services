{
    "name": "sentinel/services",
    "description": "Sentinel Services package on which all services depend",
    "keywords": [
        "sentinel",
        "services",
        "laravel",
        "api"
    ],
    "authors": [
        {
            "name": "Manuel Christlieb",
            "email": "christlieb@team23.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Sentinel\\Services\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sentinel\\Services\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sentinel\\Services\\SentinelServicesServiceProvider"
            ]
        }
    }
}
